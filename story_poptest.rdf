    </owl:NamedIndividual>
    


    <!-- http://www.florianrieder.com/semantic/story.owl#alexandre%20durand -->

    <owl:NamedIndividual rdf:about="http://www.florianrieder.com/semantic/story.owl#alexandre%20durand">
