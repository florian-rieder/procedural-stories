        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:comment xml:lang="fr">Une action que peut prendre un personnage</rdfs:comment>
        <rdfs:label xml:lang="fr">Action</rdfs:label>
    </owl:Class>
    


