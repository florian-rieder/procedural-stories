<owl:Class rdf:about="#CurrentLocation">
  <owl:equivalentClass>
    <owl:Class>
      <owl:intersectionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Location"/>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#containsCharacter"/>
          <owl:someValuesFrom rdf:resource="#Player"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
  <rdfs:subClassOf rdf:resource="#Location"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The location containing the player</rdfs:comment>
</owl:Class>

