</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#ownsItem">
  <rdfs:domain rdf:resource="#Character"/>
  <rdfs:range rdf:resource="#Item"/>
  <owl:inverseOf rdf:resource="#ownedByCharacter"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">possède</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#performedByCharacter">
  <rdfs:domain rdf:resource="#Action"/>
  <rdfs:range rdf:resource="#Character"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">est exécuté par</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#requiresItem">
  <rdfs:domain rdf:resource="#Action"/>
  <rdfs:range rdf:resource="#Item"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">requiert l'objet</rdfs:label>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#hasStanceTowardsPlayer">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a une attitude envers le joueur</rdfs:label>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasName">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">est nommé</rdfs:label>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasDescription">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">est décrit comme</rdfs:label>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Entity">
