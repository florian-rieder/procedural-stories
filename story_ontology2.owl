</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasRelationshipWith">
  <rdfs:domain rdf:resource="#Character"/>
  <rdfs:range rdf:resource="#Character"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a une relation avec</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasRole">
  <rdfs:domain rdf:resource="#Character"/>
  <rdfs:range rdf:resource="#Role"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a le rôle de</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasTrigger">
  <rdfs:domain rdf:resource="#Event"/>
  <rdfs:range rdf:resource="#Condition"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">est déclenché par</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#involvesCharacter">
  <rdfs:domain rdf:resource="#Event"/>
  <rdfs:range rdf:resource="#Character"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">implique le personnage</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isAtLocation">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Character"/>
  <rdfs:range rdf:resource="#Location"/>
  <owl:inverseOf rdf:resource="#containsCharacter"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">est situé à</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasVisited">
  <rdfs:domain rdf:resource="#Entity"/>
  <rdfs:range rdf:resource="#Location"/>
  <owl:inverseOf rdf:resource="#wasVisitedBy"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">a visité</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#wasVisitedBy">
  <rdfs:domain rdf:resource="#Location"/>
  <rdfs:range rdf:resource="#Entity"/>
  <owl:inverseOf rdf:resource="#hasVisited"/>
