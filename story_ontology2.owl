</owl:Class>

<owl:Class rdf:about="#Action">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Action</rdfs:label>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Une action que peut prendre un personnage</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Event">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Evènement</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Consequence">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Conséquence</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Emotion">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Emotion</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Goal">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Objectif</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#PersonalityTrait">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Trait de personnalité</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Role">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Role</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Condition">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Condition</rdfs:label>
</owl:Class>

<owl:Class rdf:about="#Information">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
