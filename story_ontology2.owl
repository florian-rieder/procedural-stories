  <rdfs:domain rdf:resource="#Location"/>
  <rdfs:range rdf:resource="#Location"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">est connecté à</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#ownedByCharacter">
  <rdfs:domain rdf:resource="#Item"/>
  <rdfs:range rdf:resource="#Character"/>
  <owl:inverseOf rdf:resource="#ownsItem"/>
