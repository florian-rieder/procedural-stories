        </owl:Class>
        <owl:Restriction>
          <owl:onProperty rdf:resource="#characterIsLocatedAt"/>
          <owl:someValuesFrom rdf:resource="#CurrentLocation"/>
        </owl:Restriction>
      </owl:intersectionOf>
    </owl:Class>
  </owl:equivalentClass>
