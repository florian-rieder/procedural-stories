        <hasName>Compas enchanté</hasName>
        <rdfs:label>Compas enchanté</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.florianrieder.com/semantic/story.owl#couteau%20de%20pirate -->

