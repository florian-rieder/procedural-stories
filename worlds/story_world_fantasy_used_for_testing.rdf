        <itemIsLocatedAt rdf:resource="http://www.florianrieder.com/semantic/story.owl#montagnes%20de%20la%20pierre"/>
        <hasDescription>Une lampe solide et fiable qui peut éclairer les tunnels sombres des Montagnes de la Pierre. Elle est essentielle pour naviguer dans les mines sans se perdre ou tomber dans les puits.</hasDescription>
        <hasImportance>minor</hasImportance>
        <hasName>Lampe de mineur</hasName>
        <rdfs:label>Lampe de mineur</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.florianrieder.com/semantic/story.owl#lila%20la%20courti%C3%A8re -->

    <owl:NamedIndividual rdf:about="http://www.florianrieder.com/semantic/story.owl#lila%20la%20courti%C3%A8re">
