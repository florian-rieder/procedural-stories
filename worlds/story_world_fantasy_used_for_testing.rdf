        <itemIsLocatedAt rdf:resource="http://www.florianrieder.com/semantic/story.owl#village%20des%20nains"/>
        <hasDescription>Une lampe à huile qui peut être utilisée pour éclairer les tunnels et les mines, réduisant ainsi les risques d&apos;accidents et de rencontres avec des créatures hostiles.</hasDescription>
        <hasImportance>minor</hasImportance>
        <hasName>Lampe à huile</hasName>
        <rdfs:label>Lampe à huile</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://www.florianrieder.com/semantic/story.owl#lampe%20de%20mineur -->

    <owl:NamedIndividual rdf:about="http://www.florianrieder.com/semantic/story.owl#lampe%20de%20mineur">
