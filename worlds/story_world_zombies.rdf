  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un plan détaillé de La Ville de l'Espoir, indiquant les quartiers, les routes et les points de défense.</hasDescription>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minor</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#aur%C3%A9lien%20dubois">
  <rdf:type rdf:resource="#Character"/>
  <characterIsLocatedAt rdf:resource="#la%20ville%20de%20l%27espoir"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aurélien Dubois</hasName>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aurélien Dubois</rdfs:label>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un membre de la communauté de La Ville de l'Espoir, Aurélien est responsable de la défense de la ville. Il est prêt à partager des informations sur les mouvements des Errants et les meilleures routes à prendre pour rejoindre le laboratoire de recherche.</hasDescription>
  <hasRole rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Garde</hasRole>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">secondary</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#lena%20dumont">
  <rdf:type rdf:resource="#Character"/>
  <characterIsLocatedAt rdf:resource="#la%20ville%20de%20l%27espoir"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lena Dumont</hasName>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lena Dumont</rdfs:label>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Une jeune femme qui a perdu sa famille lors de l'épidémie, Lena est devenue une experte en matière de survie et de réparation d'objets. Elle est prête à échanger des biens et des services avec le joueur.</hasDescription>
  <hasRole rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Marchande</hasRole>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">secondary</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#un%20sac%20de%20vivres">
  <rdf:type rdf:resource="#Item"/>
  <itemIsLocatedAt rdf:resource="#la%20ville%20de%20l%27espoir"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un sac de vivres</hasName>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un sac de vivres</rdfs:label>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un sac contenant des rations de survie, des fruits et des légumes frais, ainsi que quelques outils de base.</hasDescription>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minor</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#un%20outil%20de%20r%C3%A9paration">
