  <characterIsLocatedAt rdf:resource="#le%20camp%20des%20%C3%A9trangers"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aurèle Dumont</hasName>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Aurèle Dumont</rdfs:label>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un survivant expérimenté qui a perdu sa famille lors de l'épidémie. Il est prêt à tout pour protéger les siens et est ouvert à une alliance avec le joueur.</hasDescription>
  <hasRole rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Allié</hasRole>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">secondary</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#luna%20rousse">
  <rdf:type rdf:resource="#Character"/>
  <characterIsLocatedAt rdf:resource="#le%20camp%20des%20%C3%A9trangers"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Luna Rousse</hasName>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Luna Rousse</rdfs:label>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Une jeune survivante blessée qui a été sauvée par Marcus Reed. Elle est faible et a besoin d'aide, mais elle connaît des informations sur les mouvements des Errants dans la région.</hasDescription>
  <hasRole rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Blessée</hasRole>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">secondary</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#un%20sac%20de%20provisions">
  <rdf:type rdf:resource="#Item"/>
  <itemIsLocatedAt rdf:resource="#le%20camp%20des%20%C3%A9trangers"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un sac de provisions</hasName>
