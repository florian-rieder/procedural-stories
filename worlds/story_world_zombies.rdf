  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un ancien mécanicien qui vend des pièces détachées et des outils de réparation. Il peut aider le joueur à réparer son équipement.</hasDescription>
  <hasRole rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Marchand</hasRole>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">secondary</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#un%20sac%20de%20m%C3%A9dicaments">
  <rdf:type rdf:resource="#Item"/>
  <itemIsLocatedAt rdf:resource="#le%20march%C3%A9%20noir"/>
