  <hasCondition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Le joueur explore la forêt sombre.</hasCondition>
  <hasConsequence rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Le joueur doit fuir ou se battre pour survivre.</hasConsequence>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#La Découverte du laboratoire">
