  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minor</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#un%20pistolet%20%C3%A0%20air%20comprim%C3%A9">
