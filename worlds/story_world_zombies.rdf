  <hasCondition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Le joueur arrive au camp des étrangers.</hasCondition>
  <hasConsequence rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Marcus propose une alliance au joueur, mais ses intentions sont inconnues.</hasConsequence>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#L'Attaque de l'Errant Alpha">
  <rdf:type rdf:resource="#Event"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">L'Attaque de l'Errant Alpha</hasName>
