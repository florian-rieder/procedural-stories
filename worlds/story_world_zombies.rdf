  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un sac contenant des rations de nourriture et des eaux de vie. Il peut aider le joueur à survivre pendant quelques jours.</hasDescription>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minor</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#une%20carte%20de%20la%20r%C3%A9gion">
