  <itemIsLocatedAt rdf:resource="#le%20camp%20des%20%C3%A9trangers"/>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un outil de réparation</rdfs:label>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un outil de réparation</hasName>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un outil qui permet de réparer des équipements et des armes. Il peut être utile pour le joueur pour se défendre contre les Errants.</hasDescription>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minor</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#antoine%20lefebvre">
  <rdf:type rdf:resource="#Character"/>
  <characterIsLocatedAt rdf:resource="#le%20laboratoire%20de%20recherche"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Antoine Lefebvre</hasName>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Antoine Lefebvre</rdfs:label>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un technicien qui travaillait au laboratoire avant l'épidémie. Il a réussi à survivre et à se cacher dans le laboratoire. Il pourrait avoir des informations sur le remède et les équipements du laboratoire.</hasDescription>
  <hasRole rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Informateur</hasRole>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">secondary</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#sandra%20rousseau">
  <rdf:type rdf:resource="#Character"/>
  <characterIsLocatedAt rdf:resource="#le%20laboratoire%20de%20recherche"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sandra Rousseau</hasName>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Sandra Rousseau</rdfs:label>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Une chercheuse qui a travaillé avec Dr. Élise Dupont sur le remède. Elle est blessée et cache dans le laboratoire. Elle pourrait donner des informations sur le remède et le laboratoire.</hasDescription>
  <hasRole rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Blessée</hasRole>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">secondary</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#un%20ordinateur%20portable">
  <rdf:type rdf:resource="#Item"/>
  <itemIsLocatedAt rdf:resource="#le%20laboratoire%20de%20recherche"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un ordinateur portable</hasName>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un ordinateur portable</rdfs:label>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un ordinateur portable qui contient des données sur le remède et les recherches menées au laboratoire.</hasDescription>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minor</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#un%20%C3%A9chantillon%20de%20sang">
  <rdf:type rdf:resource="#Item"/>
  <itemIsLocatedAt rdf:resource="#le%20laboratoire%20de%20recherche"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un échantillon de sang</hasName>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un échantillon de sang</rdfs:label>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un échantillon de sang contaminé qui pourrait être utilisé pour comprendre l'épidémie et développer un remède.</hasDescription>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minor</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#un%20%C3%A9quipement%20de%20protection">
  <rdf:type rdf:resource="#Item"/>
  <itemIsLocatedAt rdf:resource="#le%20laboratoire%20de%20recherche"/>
  <hasName rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un équipement de protection</hasName>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un équipement de protection</rdfs:label>
  <hasDescription rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Un équipement de protection qui pourrait aider à se protéger contre les Errants.</hasDescription>
  <hasImportance rdf:datatype="http://www.w3.org/2001/XMLSchema#string">minor</hasImportance>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#aur%C3%A8le%20dumont">
